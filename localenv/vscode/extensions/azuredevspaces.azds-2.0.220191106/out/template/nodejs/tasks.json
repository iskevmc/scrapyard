{
    "AZDS-LAUNCH": [
        {
            "label": "azds: prelaunch",
            "command": "azds",
            "args": [
                "up",
                "--port=$randomPort1$:9229",
                "--await-exec",
                "--keep-alive"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": []
        }
    ],
    "AZDS-WATCH-ATTACH": [
        {
            "label": "azds: preattach",
            "command": "azds",
            "args": [
                "up",
                "--port=$randomPort1$:9229",
                "--sync-only",
                "--keep-alive",
                "--command=node_modules/.bin/nodemon",
                "--",
                "--inspect=0.0.0.0:9229",
                "--nolazy",
                "$mainJS$"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": []
        }
    ]
}