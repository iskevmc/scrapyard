		"graceful-fs": "^4.1.15",
		"lodash.merge": "^4.6.2",
		"markdownlint": "^0.11.0",
		"matcher": "^1.0.0",
		"node-dir": "^0.1.17",
		"recursive-readdir": "^2.2.2",
		"typescript-collections": "^1.2.3",
		"vscode-extension-telemetry": "^0.1.1",
		"yamljs": "^0.3.0",
		"js-yaml": "^3.13.1"
	},
	"devDependencies": {
		"@types/graceful-fs": "^2.0.30",
		"@types/jest": "24.0.12",
		"@types/lodash.merge": "^4.6.3",
		"@types/node": "^7.10.5",
		"@types/node-dir": "0.0.32",
		"@types/recursive-readdir": "^2.2.0",
		"@types/yamljs": "^0.2.30",
		"jest": "^24.7.1",
		"jest-junit": "^6.3.0",
		"octicons": "^7.3.0",
		"ts-jest": "^24.0.2",
		"tslint": "^5.9.1",
		"typescript": "3.4.5",
		"vscode": "^1.1.33"
	},
	"jest": {
		"preset": "ts-jest",
		"roots": [
			"<rootDir>/test"
		],
		"moduleFileExtensions": [
			"ts",
			"js",
			"json",
			"node"
		],
		"coverageReporters": [
			"text",
			"cobertura"
		],
		"reporters": [
			"default",
			"jest-junit"
		],
		"collectCoverage": true
	},
	"jest-junit": {
		"output": "test-results-jest.xml",
		"usePathForSuiteName": "true"
	},
